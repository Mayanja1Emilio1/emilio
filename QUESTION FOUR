#include <stdlib.h>
void addarrays(int a[6],int b[6]);
int main()
{
    int a[6] ={1 6, 67, 88, 79,10,50};
int b[6] ={7 6, 47, 8, 52,  99,110};
addarrays(a,b);
return 0;
}
void addarrays(int a[6], int b[6])
{

int c[6];
int i;
for(i=0;i<=6; i++)
{
    c[i] = b[i] + a[i];
printf("the sum is %d\n",c[i]);
}
return 0;
}

